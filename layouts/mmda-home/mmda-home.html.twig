{#
/**
 * @file
 * Template for the home page.
 */
#}
{{ attach_library('teka_mmda/greensock') }}
<div class="l-mmda-home clearfix scene_element scene_element--fadein">
  {% if content.one %}
  <div class="l-mmda-home__row l-mmda-home__row--1 parallax-hero-trigger">
    <div class="sticky-header-trigger-2"></div>
    <div class="hero">
      {{ content.one }}
    </div>
  </div>
  {% endif %}

  {% if content.two %}
    <div class="l-mmda-home__row l-mmda-home__row--2" id="about">
      <div class="l-container">
        <div class="l-container-inner">
          {{ content.two }}
        </div>
      </div>
    </div>
  {% endif %}

  {% if content.three %}
    <div class="l-mmda-home__row l-mmda-home__row--3">
      {{ content.three }}
    </div>
  {% endif %}

  {% if content.four %}
    <div class="l-mmda-home__row l-mmda-home__row--4">
      {{ content.four }}
    </div>
  {% endif %}

  {% if content.five %}
    <div class="l-mmda-home__row l-mmda-home__row--5">
      <div class="parallax-wrapper">
        <div class="parallax-inner parallax-bg">
        </div>
      </div>
      <div class="l-container">
        <div class="l-container-inner">
          {{ content.five }}
        </div>
      </div>
    </div>
  {% endif %}

  {% if content.six %}
    <div class="l-mmda-home__row l-mmda-home__row--6">
      <div class="l-container">
        <div class="l-container-inner">
          {{ content.six }}
        </div>
      </div>
    </div>
  {% endif %}
</div>
