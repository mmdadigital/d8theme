{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
{%
  set title_classes = [
  'block__title',
  ]
%}

<div{{ attributes.addClass(classes) }}>
    {{ title_prefix }}
    {% if label %}
      <h2{{ title_attributes.addClass(title_classes) }}>{{ label }}</h2>
    {% endif %}
    {{ title_suffix }}
    <div class="block__content">
      {% block content %}
        {{ content }}
      {% endblock %}
    </div>
</div>
