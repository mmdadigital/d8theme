{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
{%
  set title_classes = [
  'block__title',
  ]
%}

<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <div class="l-container">
      <div class="l-container-inner">
        <div class="block__title-wrapper">
          <h2{{ title_attributes.addClass(title_classes) }}>{{ label }}</h2>
        </div>
      </div>
    </div>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
  <div class="l-container">
    <div class="l-container-inner l-container--btn">
      <a href="{{path('page_manager.page_view_work')}}" class="btn-all-work"> {% trans %} All work {% endtrans %} </a>
    </div>
  </div>
</div>
